<p id="notice"><%= notice %></p>

<h1>Supply Portfolios</h1>

<table>
  <thead>
    <tr>
      <th>Company code</th>
      <th>Code</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @supply_portfolios.each do |supply_portfolio| %>
      <tr>
        <td><%= supply_portfolio.company_code %></td>
        <td><%= supply_portfolio.code %></td>
        <td><%= supply_portfolio.state %></td>
        <td><%= link_to 'Show', supply_portfolio %></td>
        <td><%= link_to 'Edit', edit_supply_portfolio_path(supply_portfolio) %></td>
        <td><%= link_to 'Destroy', supply_portfolio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Supply Portfolio', new_supply_portfolio_path %>
