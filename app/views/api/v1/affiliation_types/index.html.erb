<p id="notice"><%= notice %></p>

<h1>Affiliation Types</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @affiliation_types.each do |affiliation_type| %>
      <tr>
        <td><%= affiliation_type.code %></td>
        <td><%= affiliation_type.description %></td>
        <td><%= link_to 'Show', affiliation_type %></td>
        <td><%= link_to 'Edit', edit_affiliation_type_path(affiliation_type) %></td>
        <td><%= link_to 'Destroy', affiliation_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Affiliation Type', new_affiliation_type_path %>
